{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    // "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "components/*": ["./src/components/*"],
      "screens/*": ["./src/screens/*"],
      "config/*": ["./src/config/*"],
      "assets/*": ["./src/assets/*"]
    }
  },
  
  "exclude": ["node_modules"],
}


